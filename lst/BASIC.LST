   104                              <1> %macro makeSpaceOnStack 1
   105                              <1>     ;The argument is to specify how much space to make on the stack
   106                              <1>     ;It will check to ensure that we dont go past the stack minimum.
   107                              <1>     ;If we do, call stack overflow!
   108                              <1>     pop rax    ;Get the return address qword into rax
   109                              <1>     shl %1, 3  ;Multiply argument register by 8
   110                              <1>     sub rsp, %1 ;Clear space on the stack plus one qword for ret addr
   111                              <1>     lea rbp, stack
   112                              <1>     cmp rsp, rbp
   113                              <1>     ja .macroNotOverflow ;If rsp above overflow buffer, its oki
   114                              <1>     lea rbp, stkOvflw
   115                              <1>     jmp criticalError
   116                              <1> .macroNotOverflow:
   117                              <1>     ;rsp now points to the start of the cleared space
   118                              <1>     ;Put the return address back on the stack
   119                              <1>     push rax
   120                              <1> %endmacro
   121                              <1> 
   122                              <1> %macro cleanSpaceOnStack 1
